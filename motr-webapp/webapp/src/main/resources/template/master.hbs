<body class="motr ">
    {{#shouldUseAnalytics}}
        <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-TVLB97T" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    {{/shouldUseAnalytics}}
    <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>

    <!-- skiplink -->
    <div id="skiplink-container">
      <div><a href="#content" class="skiplink">Skip to main content</a></div>
    </div>

    {{> partial/cookie-preferences-banner }}

    <!-- header -->
    <header id="global-header" class="with-proposition">
        <div class="header-wrapper">
            <div class="header-global">
                <div class="header-logo">
                    <a href="https://www.gov.uk" title="Go to the GOV.UK homepage" id="logo" class="content">
                        <img src="{{{asset this 'images/gov.uk_logotype_crown_invert_trans.png?0.22.1'}}}" width="36" height="32" alt="GOV UK Crown">
                        GOV.UK
                    </a>
                </div>
            </div>

            <div class="header-proposition">
                <div class="content">
                    <nav id="proposition-menu">
                        <a href="https://www.gov.uk/mot-reminder" id="proposition-name">Get an annual MOT reminder</a>
                    </nav>
                </div>
            </div>

        </div>
    </header>
    <!-- header bar -->
    <div id="global-header-bar"></div>

    <!-- main content -->
        <main id="content">

            <div class="phase-banner">
                <p>
                    <strong class="phase-tag">BETA</strong>
                    <span>This is a new service â€“ your <a href="{{#if smartSurveyFeedback}}{{smartSurveyFeedback}}{{else}}http://www.smartsurvey.co.uk/s/MKVXI/{{/if}}" target="_blank">feedback</a> will help us to improve it.</span>
                </p>
            </div>

            {{#block "content"}}

            {{/block}}

        </main>
    <!-- / #content -->

    <!-- footer -->
        <footer class="group js-footer" id="footer">
            <div class="footer-wrapper">
                <div class="footer-meta">
                    <div class="footer-meta-inner">
                        <h2 class="visuallyhidden">Support links</h2>
                        <ul>
                            <li><a href="{{{url this "/cookies"}}}" id="cookiesLink">Cookies</a></li>
                            <li><a href="{{{url this "/terms-and-conditions"}}}" id="terms-and-conditions-link">Terms and conditions</a></li>
                            <li><a href="https://www.gov.uk/dvsa/privacy" target="_blank" id="privacy-notice-link">Privacy notice</a></li>
                            <li><a href="{{{url this "/accessibility-statement"}}}" id="accessibility-statement-link">Accessibility Statement</a></li>
                            <li>Built by the <a href="https://www.gov.uk/government/organisations/driver-and-vehicle-standards-agency">Driver &amp;
                                Vehicle Standards Agency</a></li>
                            <li class="visuallyhidden" tabindex="-1">Release {{{release-version}}}</li>
                        </ul>
                        <div class="open-government-licence">
